{
  "name": "wrfxweb",
  "private": true,
  "version": "0.0.0",
  "description": "A visualization server for the outputs of fire-weather model wrf-sfire as generated by *wrfxpy*",
  "license": "SJSU",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "http-server": "^14.1.1",
    "jasmine-core": "^2.3.4",
    "jest": "^29.7.0"
  },
  "scripts": {
    "prestart": "npm install",
    "start-client": "cd python/clientServer && ../venv/bin/flask --app __init__ run --debug --port 8000",
    "start-upload-queue": "cd python/uploadQueueService && ../venv/bin/flask --app __init__ run --debug --port 8001",
    "start-upload-worker": "cd python/uploadWorker && ../venv/bin/flask --app __init__ run --debug --port 8002",
    "test": "jest --silent"
  }
}
